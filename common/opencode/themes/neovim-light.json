{
  "$schema": "https://opencode.ai/theme.json",
  "defs": {
    "bg0": "#e0e2ea",
    "bg1": "#d7d8db",
    "fg0": "#07080d",
    "fg1": "#55585d",
    "black": "#07080d",
    "red": "#590008",
    "green": "#005523",
    "yellow": "#6b5300",
    "blue": "#004c73",
    "magenta": "#470045",
    "cyan": "#007373",
    "selection": "#f5f5f5",
    "deltaMinus": "#ffe8eb",
    "deltaMinusEmph": "#ffb3c6",
    "deltaLineMinus": "#761e1e",
    "deltaPlus": "#e6f9f0",
    "deltaPlusEmph": "#3d8259",
    "deltaLinePlus": "#a8e6c1"
  },
  "theme": {
    "primary": { "dark": "blue", "light": "blue" },
    "secondary": { "dark": "magenta", "light": "magenta" },
    "accent": { "dark": "cyan", "light": "cyan" },
    "error": { "dark": "red", "light": "red" },
    "warning": { "dark": "yellow", "light": "yellow" },
    "success": { "dark": "green", "light": "green" },
    "info": { "dark": "blue", "light": "blue" },
    "text": { "dark": "fg0", "light": "fg0" },
    "textMuted": { "dark": "fg1", "light": "fg1" },
    "background": { "dark": "bg0", "light": "bg0" },
    "backgroundPanel": { "dark": "bg1", "light": "bg1" },
    "backgroundElement": { "dark": "bg1", "light": "bg1" },
    "border": { "dark": "fg1", "light": "fg1" },
    "borderActive": { "dark": "blue", "light": "blue" },
    "borderSubtle": { "dark": "fg1", "light": "fg1" },
    "diffAdded": { "dark": "deltaLinePlus", "light": "deltaLinePlus" },
    "diffRemoved": { "dark": "deltaLineMinus", "light": "deltaLineMinus" },
    "diffContext": { "dark": "fg1", "light": "fg1" },
    "diffHunkHeader": { "dark": "blue", "light": "blue" },
    "diffHighlightAdded": { "dark": "deltaPlusEmph", "light": "deltaPlusEmph" },
    "diffHighlightRemoved": {
      "dark": "deltaMinusEmph",
      "light": "deltaMinusEmph"
    },
    "diffAddedBg": { "dark": "deltaPlus", "light": "deltaPlus" },
    "diffRemovedBg": { "dark": "deltaMinus", "light": "deltaMinus" },
    "diffContextBg": { "dark": "bg0", "light": "bg0" },
    "markdownText": { "dark": "fg0", "light": "fg0" },
    "markdownHeading": { "dark": "blue", "light": "blue" },
    "markdownLink": { "dark": "cyan", "light": "cyan" },
    "markdownLinkText": { "dark": "magenta", "light": "magenta" },
    "markdownCode": { "dark": "green", "light": "green" },
    "markdownBlockQuote": { "dark": "fg1", "light": "fg1" },
    "markdownEmph": { "dark": "magenta", "light": "magenta" },
    "markdownStrong": { "dark": "yellow", "light": "yellow" },
    "markdownHorizontalRule": { "dark": "fg1", "light": "fg1" },
    "markdownListItem": { "dark": "blue", "light": "blue" },
    "markdownListEnumeration": { "dark": "yellow", "light": "yellow" },
    "markdownImage": { "dark": "cyan", "light": "cyan" },
    "markdownImageText": { "dark": "magenta", "light": "magenta" },
    "markdownCodeBlock": { "dark": "fg0", "light": "fg0" },
    "syntaxComment": { "dark": "fg1", "light": "fg1" },
    "syntaxKeyword": { "dark": "blue", "light": "blue" },
    "syntaxFunction": { "dark": "cyan", "light": "cyan" },
    "syntaxVariable": { "dark": "fg0", "light": "fg0" },
    "syntaxString": { "dark": "green", "light": "green" },
    "syntaxNumber": { "dark": "yellow", "light": "yellow" },
    "syntaxType": { "dark": "magenta", "light": "magenta" },
    "syntaxOperator": { "dark": "fg0", "light": "fg0" },
    "syntaxPunctuation": { "dark": "fg0", "light": "fg0" }
  }
}
